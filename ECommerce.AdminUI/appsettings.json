{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Redis": {
    "ConnectionString": "${REDIS_CONNECTION_STRING}"
  },
  "OpenTelemetry": {
    "ServiceName": "AdminUI",
    "ServiceVersion": "1.0.0",
    "Exporters": {
      "Prometheus": {
        "Enabled": true,
        "Endpoint": "/metrics"
      },
      "AzureMonitor": {
        "Enabled": false,
        "ConnectionString": ""
      }
    },
    "Instrumentation": {
      "AspNetCore": true,
      "HttpClient": true,
      "SqlClient": true
    }
  },
  "AllowedHosts": "*"
}
